@model WordsBGood.Domain.Entities.Category

@{
    ViewBag.Title = "AddCategory";
}

<h2>AddCategory</h2>

<p>What subject would you like to add @Model.User.Name?</p>

@Html.ValidationSummary()

 @using (Html.BeginForm())
        {
            <p>Category to Add: @Html.TextBoxFor(x => x.Subject)</p>
            
            
 
            <input type ="submit" value ="Add Category" />
        }
