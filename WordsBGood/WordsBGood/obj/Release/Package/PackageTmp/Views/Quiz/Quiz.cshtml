@model WordsBGood.Domain.Entities.Quiz

@{
    ViewBag.Title = "Quiz";
}

<h2>Quiz</h2>


 @using (Html.BeginForm())
 {
    foreach (var v in Model.Category.VocabList)
    {
        <p>@v.Definition</p>
        @Html.TextBoxFor(x => x.Answer)  <br />
        
    }
    <input type ="submit" value ="Submit Answers" />
 }