@model WordsBGood.Domain.Entities.Quiz

@{
    ViewBag.Title = "QuizResults";
}

<h2>QuizResults</h2>

You scored: @Model.Score

<p>These are the words you got correct:</p>
@foreach (var c in Model.Correct)
{
    @c.Word @:,
}
<p>These are the words you need to work on:</p>
@foreach (var i in Model.Incorrect)
{
    @i.Word @:,
}
